
public class JavaWalkthrough {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int age = 56 ,userID = 88;  // declaring multiple variables 

		System.out.println(  "Your age is "    + age + "\n" + " and your userID is  " +"\n  "
				+ "Now ,  " +  userID +  " \n "
						+ " These are examples of int or integer data types  , they are 4 bits long");
		
		System.out.println( " Both float and double variables , can only store floating point numbers " + 
		"\n" + "The size of a float is four bytes , while a double , holding larger values is 8 bytes. \n"
				+ "this means odubles can store numbers with better precision.  ");
		
		
		float Floater = 10.2f;  // - Note , a floating point number will have a f at the end
		double ddouble = 55.5545;
		
		System.out.println(" THis is an example of a float variable " + Floater  + "\n" 
				+ " and this , a double , " + ddouble);
		
		System.out.println( "THe next number is a exponential number , they are written like ,"+  "\n" + " double factor \' - 22.46e6 \'  as if to be to the sixth exponent.    " );
		double exponentialNumber = 1000e20;
		System.out.println("One big assed exponential  number is equal too  - " + exponentialNumber);
		
		System.out.println("The next example is a character data type , a char contians 4 bytes , \n "
				+ "we use single quotes or '' to represent a character. \n"
				+ "the char variable is used for characters , its size is 2 bytes.  ");   
		char firstCharacter = 'M'; 
		System.out.printf("helloWorld " + firstCharacter);
		
		
		
		
		
		
		
		
		
		
		//System.out.println( "  " )
		
		//System.out.println()
		
		
		
		
	}

}
